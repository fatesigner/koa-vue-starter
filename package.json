  "license": "MIT",
  "keywords": [
    "demo",
    "semantic",
    "release"
  ],
  "author": {
    "name": "Jeremy Tang",
    "email": "forgleaner@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forgleaner/koa-vue-starter.git"
  },
  "bugs": {
    "url": "https://github.com/forgleaner/koa-vue-starter/issues"
  },
  "homepage": "https://github.com/forgleaner/koa-vue-starter#readme",
  "scripts": {
    "clean": "gulp clean --gulpfile build/tasks.js",
    "server:start": "ts-node --project tsconfig.server.json src/server/server.ts",
    "server:build": "gulp server:build --gulpfile build/tasks.js && tsc -p tsconfig.server.json",
    "server:watch": "nodemon",
    "client:build": "gulp client:build --gulpfile build/tasks.js",
    "client:watch": "gulp client:watch --gulpfile build/tasks.js",
    "client:prod": "cross-env NODE_ENV=production gulp client:build --gulpfile build/tasks.js",
    "build": "cross-env-shell NODE_ENV=production \"npm run client:build && npm run server:build && node ./bin/server/server.js\"",
    "start": "cross-env-shell NODE_HOTRELOAD=true \"nodemon\"",
    "watch": "cross-env-shell NODE_WATCH=true \"npm run client:watch\"",
    "lint": "lint-staged",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "prettier": "@forgleaner/eslint-config/.prettierrc",
  "dependencies": {
    "@forgleaner/sass-helpers": "1.0.0",
    "@forgleaner/utils": "1.0.0",
