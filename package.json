    "koa-onerror": "4.1.0",
    "koa-redis": "4.0.1",
    "koa-router": "7.4.0",
    "koa-send": "5.0.0",
    "koa-session": "5.13.1",
    "koa-static": "5.0.0",
    "koa-views": "6.2.1",
    "koa-webpack-dev-middleware": "2.0.2",
    "koa-webpack-hot-middleware": "1.0.3",
